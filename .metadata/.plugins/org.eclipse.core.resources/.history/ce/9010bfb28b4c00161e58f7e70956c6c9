package 실험용패키지2;

public class Person2 {

	String name;
	int age;
	
//	멤버에 값을 저장해주는 메소드 : setter
	public void setName(String name){
		this.name = name;
	}
	public void setAge(int age){
		this.age = age;
	}
	
//	멤버에 값을 알려주는 메소드 : getter
	public String getName(){
		return this.name;
	}
	public int getAge(){
		return this.age;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
}

